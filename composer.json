{
    "name": "vipertecpro/lumen-log-viewer",
    "description": "A Lumen log reader",
    "license": "MIT",
    "keywords": ["log", "log-reader", "log-viewer", "logging", "lumen"],
    "type": "laravel-package",
    "authors": [
        {
            "name": "vipertecpro",
            "email": "vipertecpro@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "illuminate/support": "6.*"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "orchestra/testbench": "~3.8.0|^4.0",
        "phpunit/phpunit" : "^8.0"
    },
    "autoload": {
        "psr-4": {
            "LumenLogViewer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LumenLogViewer\\Test\\": "tests/"
        }
    },
    "minimum-stability": "stable"
}
